<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffffff" >

    <RelativeLayout
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true" >

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/btn_back" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/rl_top"
        android:layout_marginLeft="20dp" >

        <TextView
            android:id="@+id/tv_button"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:text="Button"
            android:textColor="#ff00ff00"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/tv_high"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@id/tv_button"
            android:text="High"
            android:textColor="#ff00ff00"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/tv_low"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@id/tv_high"
            android:text="Low"
            android:textColor="#ff00ff00"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/tv_dalay"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@id/tv_low"
            android:text="Dalay"
            android:textColor="#ff00ff00"
            android:textSize="22sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_content"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/rl_title"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp" >
    </RelativeLayout>

    <TableLayout
        android:id="@+id/tl_content"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/rl_title"
        android:layout_marginTop="20dp" >

        <TableRow
            android:id="@+id/tr_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/tv_1_1"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:text="A"
                android:textStyle="bold"
                android:textColor="#ff000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_1_2"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_1_3"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_1_4"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/tv_2_1"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:text="B"
                android:textStyle="bold"
                android:textColor="#ff000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_2_2"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_2_3"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_2_4"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/tv_3_1"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:text="C"
                android:textStyle="bold"
                android:textColor="#ff000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_3_2"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_3_3"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_3_4"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/tv_4_1"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:text="D"
                android:textStyle="bold"
                android:textColor="#ff000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv_4_2"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_4_3"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_4_4"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/tv_low"
                android:textColor="#ff000000"
                android:textSize="18sp" />
        </TableRow>
    </TableLayout>

</RelativeLayout>